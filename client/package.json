{
    "name": "ccm",
    "version": "1.0.0",
    "description": "Code Clubs Manager",
    "scripts": {
        "start-message": "babel-node tools/startMessage.js",
        "prestart": "npm-run-all --parallel start-message remove-dist",
        "start": "npm-run-all --parallel test:watch open:src lint:watch",
        "open:src": "babel-node tools/srcServer.js",
        "open:dist": "babel-node tools/distServer.js",
        "lint:js": "esw webpack.config.* src tools",
        "lint:jsx": "esw webpack.config.* src --ext .jsx",
        "lint": "npm run lint:js && npm run lint:jsx",
        "lint:watch": "npm run lint -- --watch",
        "clean-dist": "npm run remove-dist && mkdir dist",
        "remove-dist": "rimraf ./dist",
        "prebuild": "npm run clean-dist && npm run lint && npm run test",
        "build": "babel-node tools/build.js",
        "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter spec",
        "test:watch": "npm run test -- --watch",
        "rest-mock": "nodemon tools/restMockServer.js",
        "readme:init": "gitbook init",
        "readme:build": "gitbook build readme/src readme/dist && browser-sync start --server ./readme/dist --port 4000"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {},
    "devDependencies": {
        "object-assign": "4.1.0",
        "react": "15.4.0",
        "react-dom": "15.4.0",
        "react-redux": "4.4.6",
        "react-router": "3.0.0",
        "redux": "3.6.0",
        "autoprefixer": "6.5.3",
        "babel-cli": "6.18.0",
        "babel-core": "6.18.2",
        "babel-loader": "6.2.8",
        "babel-plugin-react-display-name": "2.0.0",
        "babel-polyfill": "6.16.0",
        "babel-preset-es2015": "6.18.0",
        "babel-preset-react": "6.16.0",
        "babel-preset-react-hmre": "1.1.1",
        "babel-preset-stage-1": "6.16.0",
        "babel-register": "6.18.0",
        "body-parser": "1.15.2",
        "browser-sync": "2.18.2",
        "chai": "3.5.0",
        "chalk": "1.1.3",
        "cheerio": "0.22.0",
        "connect-history-api-fallback": "1.3.0",
        "coveralls": "2.11.15",
        "cross-env": "3.1.3",
        "css-loader": "0.26.0",
        "enzyme": "2.6.0",
        "eslint": "3.10.2",
        "eslint-import-resolver-webpack": "0.7.0",
        "eslint-plugin-import": "2.2.0",
        "eslint-plugin-jsx-a11y": "3.0.1",
        "eslint-plugin-react": "6.7.1",
        "eslint-watch": "2.1.14",
        "eventsource-polyfill": "0.9.6",
        "express": "4.14.0",
        "extract-text-webpack-plugin": "1.0.1",
        "file-loader": "0.9.0",
        "fixed-data-table": "0.6.3",
        "fs-extra": "1.0.0",
        "gitbook-cli": "2.3.0",
        "http-proxy-middleware": "0.17.2",
        "isomorphic-fetch": "2.2.1",
        "jsdom": "9.8.3",
        "mocha": "3.1.2",
        "nock": "9.0.2",
        "node-sass": "3.13.0",
        "nodemon": "1.11.0",
        "npm-run-all": "3.1.1",
        "postcss-loader": "1.1.1",
        "prompt": "1.0.0",
        "react-addons-css-transition-group": "15.4.0",
        "react-addons-test-utils": "15.4.0",
        "react-dimensions": "1.3.0",
        "react-fileupload": "2.4.0",
        "redux-immutable-state-invariant": "1.2.4",
        "redux-logger": "2.7.4",
        "redux-mock-store": "1.2.1",
        "redux-thunk": "2.1.0",
        "replace": "0.3.0",
        "rimraf": "2.5.4",
        "sass-loader": "4.0.2",
        "sinon": "1.17.6",
        "sinon-chai": "2.8.0",
        "style-loader": "0.13.1",
        "webpack": "1.13.3",
        "webpack-dev-middleware": "1.8.4",
        "webpack-hot-middleware": "2.13.2",
        "webpack-svgstore-plugin": "kazagkazag/webpack-svgstore-plugin"
    }
}
